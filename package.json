    "@angular/http": "^2.0.0",
    "@angular/platform-browser": "^2.0.0",
    "@angular/platform-browser-dynamic": "^2.0.0",
    "@angular/router": "^3.0.0",
    "@angularclass/api": "^1.2.0",
    "angular2-busy": "^1.0.2",
    "angular2-image-upload": "^0.3.2",
    "angular2-materialize": "^6.7.1",
    "core-js": "^2.4.1",
